    "version": "0.2.0",
    "configurations": [
        {
            "name": "C#: blackjack",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/blackjack/bin/Debug/net9.0/blackjack.dll",
            "args": [],
            "cwd": "${workspaceFolder}/blackjack",
            "stopAtEntry": false,
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart",
            "launchBrowser": {
                "enabled": false
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        }
    ]
}